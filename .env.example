# Configuration Stellaris
# Renommez ce fichier en .env et ajustez les valeurs

# Clé admin pour l'interface /api/admin
ADMIN_API_KEY=changeme

# Port HTTP du serveur
PORT=3000

# Notifications email (SMTP)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=apikey
SMTP_PASS=secret
SMTP_FROM="Stellaris <no-reply@example.com>"

# Destinataire des alertes admin (nouvelle demande de créneau)
ADMIN_NOTIFY_EMAIL=admin@example.com
